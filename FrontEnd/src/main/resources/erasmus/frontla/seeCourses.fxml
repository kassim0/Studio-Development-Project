<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="510.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="erasmus.frontla.controllers.SeeCoursesController">
   <children>
      <Button fx:id="btnSeeCourses" layoutX="200.0" layoutY="29.0" mnemonicParsing="false" onAction="#seeCourses" prefHeight="38.0" prefWidth="94.0" text="See courses" />
      <ListView fx:id="lst" layoutX="28.0" layoutY="148.0" onMouseClicked="#listViewSelectedCourse" prefHeight="311.0" prefWidth="439.0" />
      <Button fx:id="nuevaVista" layoutX="505.0" layoutY="433.0" mnemonicParsing="false" onAction="#nuevaVista" text="Select course" />
      <Label fx:id="etiqueta" layoutX="247.0" layoutY="459.0" prefHeight="37.0" prefWidth="93.0" text="Label" />
      <TextField fx:id="searchBar" layoutX="28.0" layoutY="109.0" prefHeight="26.0" prefWidth="439.0" />
      <Button fx:id="btnSearch" layoutX="479.0" layoutY="110.0" mnemonicParsing="false" onAction="#searchCourse" prefHeight="26.0" prefWidth="94.0" text="Search" />
   </children>
</AnchorPane>
